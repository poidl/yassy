@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix time:  <http://lv2plug.in/ns/ext/time#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .

<http://example.org/yassy>
	a lv2:Plugin ;
        a lv2:InstrumentPlugin

	lv2:project <http://example.org> ;

	doap:name "yassy (simple synth)" ;
	lv2:requiredFeature urid:map;
        #ui:ui <http://example.org/yassyui#kx> ;
	#ui:ui <http://example.org/yassyui#ui> ;
        lv2:port [
          a lv2:InputPort ,
                  atom:AtomPort ;
          atom:bufferType atom:Sequence ;
          atom:supports midi:MidiEvent ;
          lv2:index 0 ;
          lv2:symbol "in" ;
          lv2:name "In"
	] , [
          a lv2:AudioPort ,
                  lv2:OutputPort ;
          lv2:index 1 ;
          lv2:symbol "out" ;
          lv2:name "Out"
	] , [
	  a lv2:InputPort ,
                  lv2:ControlPort ;
          lv2:index 2 ;
          lv2:symbol "blit" ;
          lv2:name "BLIT" ;
          lv2:default 1.0 ;
          lv2:minimum 0.0 ;
          lv2:maximum 1.0 ;
          lv2:portProperty lv2:integer                   
	] , [
	  a lv2:InputPort ,
                  lv2:ControlPort ;
          lv2:index 3 ;
          lv2:symbol "polyphony" ;
          lv2:name "Polyphony" ;
          lv2:default 0.0 ;
          lv2:minimum 0.0 ;
          lv2:maximum 1.0 ;
          lv2:portProperty lv2:integer                   
	] , [
	  a lv2:InputPort ,
                  lv2:ControlPort ;
          lv2:index 4 ;
          lv2:symbol "unison" ;
          lv2:name "Unison" ;
          lv2:default 0.0 ;
          lv2:minimum 0.0 ;
          lv2:maximum 1.0 ;
          lv2:portProperty lv2:integer                   
	] , [
	  a lv2:InputPort ,
                  lv2:ControlPort ;
          lv2:index 5 ;
          lv2:symbol "voices" ;
          lv2:name "Voices" ;
          lv2:default 1.0 ;
          lv2:minimum 1.0 ;
          lv2:maximum 11.0 ;
          lv2:portProperty lv2:integer                   
  ] .
        